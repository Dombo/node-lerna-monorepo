{
  "presets": ["@babel/env", "@babel/react"],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        [
          "env",
          {
            "modules": "commonjs"
          }
        ]
      ],
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
